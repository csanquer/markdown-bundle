{
    "name": "csanquer/parsedown-bundle",
    "description": "Symfony2 bundle to parse github flavour markdown with syntax highlighting using Parsedown",
    "keywords": ["parsedown", "markdown", "syntax highlighting", "geshi", "pygments", "pygmentize"],
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Sanquer",
            "email": "charles.sanquer@gmail.com",
            "role": "developer"
        }
    ],
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "CSanquer\\Bundle\\ParsedownBundle\\": ""
        }
    },
    "require": {
        "symfony/framework-bundle": "~2.1",
        "erusev/parsedown": "~1.0",
        "easybook/geshi": "~1.0",
        "doctrine/cache": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "suggest": {
        "erusev/parsedown-extra": "dev-master"
    },
    "minimum-stability": "dev"
}
